<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route url="/V1/xcore/paymentmethods" method="GET">
        <service class="Dealer4dealer\Xcore\Api\PaymentMethodRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="Magento_Sales::custom_attribute" />
        </resources>
    </route>
    <route url="/V1/xcore/shippingmethods" method="GET">
        <service class="Dealer4dealer\Xcore\Api\ShippingMethodRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="Magento_Sales::sales" />
        </resources>
    </route>
    <route url="/V1/xcore/customattributes" method="GET">
        <service class="Dealer4dealer\Xcore\Api\CustomAttributeRepositoryInterface" method="getList"/>
        <resources>
            <resource ref="Dealer4dealer_Xcore::custom_attribute"/>
        </resources>
    </route>
    <route url="/V1/xcore/altershipment" method="PUT">
        <service class="Dealer4dealer\Xcore\Api\AlterShipmentInterface" method="execute"/>
        <resources>
            <resource ref="Magento_Sales::sales" />
        </resources>
    </route>
    <route url="/V1/xcore/pricelist" method="GET">
        <service class="Dealer4dealer\Xcore\Api\PricelistManagementInterface" method="getPricelist"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
    <route url="/V1/xcore/pricelist" method="POST">
        <service class="Dealer4dealer\Xcore\Api\PricelistManagementInterface" method="postPricelist"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>