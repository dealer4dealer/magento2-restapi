<integrations>
    <integration name="dealer4dealer xCore">
        <resources>
            <resource name="Magento_Sales::sales">
                <resource name="Magento_Sales::sales_operation">
                    <resource name="Magento_Sales::sales_order">
                        <resource name="Magento_Sales::actions">
                            <resource name="Magento_Sales::create"/>
                            <resource name="Magento_Sales::actions_view"/>
                            <resource name="Magento_Sales::email"/>
                            <resource name="Magento_Sales::reorder"/>
                            <resource name="Magento_Sales::actions_edit"/>
                            <resource name="Magento_Sales::cancel"/>
                            <resource name="Magento_Sales::review_payment"/>
                            <resource name="Magento_Sales::capture"/>
                            <resource name="Magento_Sales::invoice"/>
                            <resource name="Magento_Sales::creditmemo"/>
                            <resource name="Magento_Sales::hold"/>
                            <resource name="Magento_Sales::unhold"/>
                            <resource name="Magento_Sales::ship"/>
                            <resource name="Magento_Sales::comment"/>
                            <resource name="Magento_Sales::emails"/>
                        </resource>
                    </resource>
                    <resource name="Magento_Sales::sales_invoice"/>
                    <resource name="Magento_Sales::shipment"/>
                    <resource name="Magento_Sales::sales_creditmemo"/>
                    <resource name="Magento_Sales::transactions">
                        <resource name="Magento_Sales::transactions_fetch"/>
                    </resource>
                </resource>
            </resource>
            <resource name="Magento_Catalog::catalog">
                <resource name="Magento_Catalog::catalog_inventory">
                    <resource name="Magento_Catalog::products">
                        <resource name="Magento_Catalog::update_attributes"/>
                        <resource name="Magento_Catalog::edit_product_design"/>
                    </resource>
                    <resource name="Magento_Catalog::categories">
                        <resource name="Magento_Catalog::edit_category_design"/>
                    </resource>
                </resource>
            </resource>
            <resource name="Magento_Customer::customer">
                <resource name="Magento_Customer::manage">
                    <resource name="Magento_Customer::actions">
                        <resource name="Magento_Customer::delete"/>
                    </resource>
                </resource>
                <resource name="Magento_Customer::group"/>
            </resource>
            <resource name="Magento_Backend::marketing">
                <resource name="Magento_CatalogRule::promo">
                    <resource name="Magento_CatalogRule::promo_catalog"/>
                </resource>
            </resource>
            <resource name="Magento_Backend::stores">
                <resource name="Magento_Backend::stores_settings">
                    <resource name="Magento_Backend::store"/>
                    <resource name="Magento_Config::config">
                        <resource name="Magento_Catalog::config_catalog"/>
                        <resource name="Magento_Customer::config_customer"/>
                        <resource name="Magento_Tax::config_tax"/>
                    </resource>
                    <resource name="Magento_Sales::order_statuses"/>
                </resource>
                <resource name="Magento_InventoryApi::inventory">
                    <resource name="Magento_InventoryApi::source">
                        <resource name="Magento_InventoryApi::source_edit"/>
                    </resource>
                    <resource name="Magento_InventoryApi::stock">
                        <resource name="Magento_InventoryApi::stock_edit"/>
                        <resource name="Magento_InventoryApi::stock_delete"/>
                        <resource name="Magento_InventoryApi::stock_source_link"/>
                    </resource>
                    <resource name="Magento_InventoryInStorePickupApi::inStorePickup">
                        <resource name="Magento_InventoryInStorePickupApi::notify_orders_are_ready_for_pickup"/>
                    </resource>
                    <resource name="Magento_InventorySalesApi::stock"/>
                </resource>
                <resource name="Magento_Tax::manage_tax"/>
                <resource name="Magento_CurrencySymbol::system_currency">
                    <resource name="Magento_CurrencySymbol::currency_rates"/>
                    <resource name="Magento_CurrencySymbol::symbols"/>
                </resource>
                <resource name="Magento_Backend::stores_attributes">
                    <resource name="Magento_Catalog::attributes_attributes"/>
                    <resource name="Magento_Catalog::sets"/>
                </resource>
                <resource name="Magento_Backend::stores_other_settings"/>
            </resource>
            <resource name="Magento_Backend::system">
                <resource name="Magento_Backend::extensions">
                    <resource name="Magento_Backend::local"/>
                    <resource name="Magento_Backend::custom"/>
                </resource>
                <resource name="Magento_Backend::tools">
                    <resource name="Magento_Backend::cache">
                        <resource name="Magento_Backend::main_actions">
                            <resource name="Magento_Backend::flush_cache_storage"/>
                            <resource name="Magento_Backend::flush_magento_cache"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </integration>
</integrations>
